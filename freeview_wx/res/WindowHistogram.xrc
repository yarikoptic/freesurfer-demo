                        <option>0</option>
                        <flag>wxALL</flag>
                        <border>5</border>
                        <object class="wxButton" name="ID_BUTTON_APPLY">
                            <label>Apply</label>
                            <default>1</default>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <option>0</option>
                        <flag>wxALL</flag>
                        <border>5</border>
                        <object class="wxButton" name="ID_BUTTON_CLOSE">
