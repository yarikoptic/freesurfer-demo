                        </object>
                    </object>
                    <object class="sizeritem">
                        <option>0</option>
                        <flag>wxALL</flag>
                        <border>5</border>
                        <object class="wxChoice" name="ID_CHOICE_LAYER">
                            <size>120,-1</size>
