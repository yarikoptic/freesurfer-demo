              <option>0</option>
              <flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
              <border>5</border>
              <object class="wxStaticText" name="ID_STATIC_BRUSH_SIZE">
                <label>Brush size</label>
              </object>
            </object>
            <object class="sizeritem">
              <option>1</option>
              <flag>wxALL</flag>
              <border>5</border>
              <object class="wxSpinCtrl" name="ID_SPIN_BRUSH_SIZE">
                <style>wxSP_ARROW_KEYS</style>
                <size>60,-1</size>
                <value>1</value>
                <min>1</min>
                <max>10</max>
              </object>
            </object>
            <object class="sizeritem">
              <option>0</option>
              <flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
              <border>5</border>
              <object class="wxStaticText" name="ID_STATIC_REFERENCE">
                <label>Reference</label>
              </object>
            </object>
