                <content />
              </object>
            </object>
            <object class="sizeritem">
              <option>0</option>
              <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
              <border>5</border>
              <object class="wxStaticText" name="ID_STATIC_TOLERANCE">
                <label>Tolerance</label>
              </object>
            </object>
