                  <option>1</option>
                  <flag>wxALIGN_CENTER_VERTICAL|wxTOP|wxBOTTOM|wxRIGHT</flag>
                  <border>5</border>
                  <object class="wxStaticText" name="ID_STATIC_PERCENTAGE">
                    <label>%</label>
                  </object>
                </object>
              </object>
            </object>
