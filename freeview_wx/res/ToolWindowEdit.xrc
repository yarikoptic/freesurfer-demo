                </object>
              </object>
            </object>
            <object class="sizeritem">
              <option>0</option>
              <flag>wxALL|wxEXPAND</flag>
              <border>5</border>
              <object class="wxStaticText" name="ID_STATIC_NOTES_CONTOUR">
