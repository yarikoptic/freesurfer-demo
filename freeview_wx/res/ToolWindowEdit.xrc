<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">
  <object class="wxFrame" name="ID_TOOLWINDOW_EDIT">
    <style>wxCAPTION|wxCLOSE_BOX|wxFRAME_FLOAT_ON_PARENT|wxFRAME_NO_TASKBAR|wxFRAME_TOOL_WINDOW</style>
    <size>343,680</size>
    <title>Drawing</title>
    <object class="wxPanel" name="ID_PANEL_HOLDER">
      <style>wxTAB_TRAVERSAL</style>
