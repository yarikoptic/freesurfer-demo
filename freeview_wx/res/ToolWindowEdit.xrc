                    <maxlength>0</maxlength>
                  </object>
                </object>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxALIGN_CENTER_VERTICAL|wxBOTTOM|wxRIGHT|wxLEFT</flag>
                  <border>5</border>
                  <object class="wxStaticText" name="ID_STATIC_DRAW_HIGH">
                    <label>   High</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxBOTTOM|wxRIGHT|wxLEFT</flag>
                  <border>5</border>
                  <object class="wxTextCtrl" name="ID_EDIT_DRAW_RANGE_HIGH">
                    <value></value>
                    <maxlength>0</maxlength>
                  </object>
                </object>
              </object>
            </object>
            <object class="sizeritem">
              <option>0</option>
              <flag>wxALL</flag>
              <border>5</border>
              <object class="wxCheckBox" name="ID_CHECK_EXCLUDE_RANGE">
                <label>Do not draw on pixel in the range of</label>
                <checked>0</checked>
              </object>
            </object>
            <object class="sizeritem">
              <option>0</option>
              <flag>wxEXPAND|wxBOTTOM</flag>
              <border>5</border>
              <object class="wxBoxSizer">
                <orient>wxHORIZONTAL</orient>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxALIGN_CENTER_VERTICAL|wxBOTTOM|wxRIGHT|wxLEFT</flag>
                  <border>5</border>
                  <object class="wxStaticText" name="ID_STATIC_EXCLUDE_LOW">
                    <label> Low</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxBOTTOM|wxRIGHT|wxLEFT</flag>
                  <border>5</border>
                  <object class="wxTextCtrl" name="ID_EDIT_EXCLUDE_RANGE_LOW">
                    <value></value>
                    <maxlength>0</maxlength>
                  </object>
                </object>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxALIGN_CENTER_VERTICAL|wxBOTTOM|wxRIGHT|wxLEFT</flag>
                  <border>5</border>
                  <object class="wxStaticText" name="ID_STATIC_EXCLUDE_HIGH">
                    <label>   High</label>
                  </object>
                </object>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxBOTTOM|wxRIGHT|wxLEFT</flag>
                  <border>5</border>
                  <object class="wxTextCtrl" name="ID_EDIT_EXCLUDE_RANGE_HIGH">
                    <value></value>
                    <maxlength>0</maxlength>
                  </object>
                </object>
              </object>
            </object>
            <object class="sizeritem">
              <option>0</option>
              <flag>wxALL</flag>
              <border>5</border>
