              <option>1</option>
              <flag>wxEXPAND</flag>
              <border>5</border>
              <object class="wxBoxSizer">
                <orient>wxHORIZONTAL</orient>
                <object class="sizeritem">
                  <option>0</option>
                  <flag>wxALL</flag>
                  <border>5</border>
                  <object class="wxSpinCtrl" name="ID_SPIN_BRUSH_TOLERANCE">
                    <style>wxSP_ARROW_KEYS</style>
                    <size>60,-1</size>
                    <value>0</value>
                    <min>0</min>
                    <max>100</max>
