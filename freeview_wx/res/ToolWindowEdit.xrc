              <option>1</option>
              <flag>wxEXPAND</flag>
              <border>5</border>
              <object class="wxToolBar" name="ID_TOOLBAR_VOXEL_EDIT">
                <bitmapsize></bitmapsize>
                <margins></margins>
                <packing>1</packing>
                <separation>5</separation>
                <object class="tool" name="ID_ACTION_VOXEL_FREEHAND">
                  <label>Freehand</label>
                  <tooltip>Freehand</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_freehand.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_VOXEL_POLYLINE">
                  <label>Polyline</label>
                  <tooltip>Polyline</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_polyline.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_VOXEL_LIVEWIRE">
                  <label>Livewire</label>
                  <tooltip>Livewire</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_livewire.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_VOXEL_FILL">
                  <label>Fill</label>
                  <tooltip>Fill</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_fill.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_VOXEL_CONTOUR">
                  <label>Contour</label>
                  <tooltip>Intensity Contour</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_contour.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="separator" />
