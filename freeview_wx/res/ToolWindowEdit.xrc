                  <label>Color Picker</label>
                  <tooltip>Color Picker</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_colorpicker.png</bitmap>
                  <toggle>1</toggle>
                </object>
              </object>
            </object>
            <object class="sizeritem">
              <option>1</option>
              <flag>wxEXPAND</flag>
              <border>5</border>
              <object class="wxToolBar" name="ID_TOOLBAR_ROI_EDIT">
                <style>wxTB_FLAT|wxTB_HORIZONTAL</style>
                <bitmapsize></bitmapsize>
                <margins></margins>
                <packing>1</packing>
                <separation>5</separation>
                <object class="tool" name="ID_ACTION_ROI_FREEHAND">
                  <label>Freehand</label>
                  <tooltip>Freehand</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_freehand.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_ROI_POLYLINE">
                  <label>Polyline</label>
                  <tooltip>Polyline</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_polyline.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_ROI_LIVEWIRE">
                  <label>Livewire</label>
                  <tooltip>Livewire</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_livewire.png</bitmap>
                  <toggle>1</toggle>
                </object>
                <object class="tool" name="ID_ACTION_ROI_FILL">
                  <label>Fill</label>
                  <tooltip>Fill</tooltip>
                  <longhelp></longhelp>
                  <bitmap>images/voxel_draw_fill.png</bitmap>
                  <toggle>1</toggle>
                </object>
              </object>
            </object>
